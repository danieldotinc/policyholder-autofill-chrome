{
  "manifest_version": 2,
  "name": "PolicyHolder-Autofill",
  "description": "Populates policyholder fields",
  "version": "1.0",
  "browser_action": {
    "default_title": "Click here to load form data!",
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"]
}
